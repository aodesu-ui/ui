      "content": "@import url('https://fonts.googleapis.com/css2?family=Amatic+SC:wght@400;700&display=swap');\n\n[data-theme=\"aodesu-light\"] {\n  --font-branch: \"Amatic SC\", sans-serif;\n\n  --background: 30 20% 95%;\n  --foreground: 0 0% 0%;\n  --primary: 30 50% 30%;\n  --primary-foreground: 0 0% 100%;\n  --secondary: 30 10% 50%;\n  --secondary-foreground: 0 0% 100%;\n  --border: 0 0% 80%;\n\n  --border-outline: 0 0% 80%;\n}\n\n[data-theme=\"aodesu-dark\"] {\n  --font-branch: \"Amatic SC\", sans-serif;\n\n  --background: 30 20% 10%;\n  --foreground: 0 0% 100%;\n  --primary: 30 50% 70%;\n  --primary-foreground: 0 0% 0%;\n  --secondary: 30 10% 30%;\n  --secondary-foreground: 0 0% 100%;\n\n  --border-outline: 0 0% 20%;\n}",
      "type": "registry:style",
      "target": "themes/aodesu.css"
    }
  ]
}